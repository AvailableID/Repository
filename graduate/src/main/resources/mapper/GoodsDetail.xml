<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yiwang.graduate.mapper.GoodsDetailMapper">
    <select id="findByGoodsId" parameterType="int" resultType="com.yiwang.graduate.entity.GoodsDetail">
        select * from goods_detail where goods_id = #{value } order by gd_order
    </select>
</mapper>